{
  "name": "lyrebird-demo-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"node src/server/mock-server.js\" \"vite\"",
    "dev:server": "node src/server/mock-server.js",
    "dev:client": "vite",
    "dev:host": "concurrently -k \"node src/server/mock-server.js\" \"vite --host 0.0.0.0 --port 5173\"",
    "testsprite:mcp": "node scripts/start-testsprite-mcp.mjs",
    "test:ui": "npx playwright test tests/localhost.spec.ts --reporter=line",
    "test:ui:debug": "npx playwright test tests/localhost-debug.spec.ts --reporter=line",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@copilotkit/react-core": "^1.51.4",
    "@copilotkit/react-ui": "^1.51.4",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.0",
    "dotenv": "^16.6.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
